{
  "Version": "1.1",
  "Statement": [
    {
      "Action": [
        "obs:*:get*",
        "obs:*:list*",
        "obs:*:head*"
      ],
      "Effect": "Allow"
    },
    {
      "Condition": {
        "StringNotEqualsIgnoreCase": {
          "g:ServiceName": [
            "iam"
          ]
        }
      },
      "Action": [
        "*:*:get*",
        "*:*:list*",
        "*:*:head*",
        "*:*:display*",
        "*:*:query*"
      ],
      "Effect": "Allow"
    },
    {
      "Action": [
        "iam:*:get*",
        "iam:*:list*",
        "iam:*:check*"
      ],
      "Effect": "Allow"
    }
  ]
}
